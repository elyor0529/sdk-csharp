<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.1.5</Version>
    <Authors>Joel Christner</Authors>
    <Description>C# SDK for Kvpbase object storage platform</Description>
    <Copyright>(c)2019 Joel Christner</Copyright>
    <RepositoryUrl>https://github.com/kvpbase/sdk-csharp</RepositoryUrl>
    <PackageProjectUrl>https://github.com/kvpbase/sdk-csharp</PackageProjectUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageLicenseUrl>https://github.com/kvpbase/sdk-csharp/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageReleaseNotes>Retarget to .NET Core 2.0 and .NET Framework 4.6.1</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="RestWrapper" Version="1.0.9" />
  </ItemGroup>

</Project>
